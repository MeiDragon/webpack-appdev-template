	},
	"bugs": {
		"url": "https://github.com/MeiDragon/webpack-appdev-template/issues"
	},
	"keywords": [
		"template",
		"webpack"
	],
	"scripts": {
		"esbuild-dev": "cross-env NODE_ENV=development Transpiler=esbuild webpack serve --config webpack.dev.ts",
		"babel-dev": "cross-env NODE_ENV=development Transpiler=babel webpack serve --config webpack.dev.ts",
		"babel-build": "cross-env NODE_ENV=production Transpiler=babel webpack --config webpack.prod.ts",
		"esbuild-build": "cross-env NODE_ENV=production Transpiler=esbuild webpack --config webpack.prod.ts",
		"style": "npx prettier --write '**/*.{js,ts,tsx,json,md,css,scss,html}'"
	},
